<div>
  <div><img src="/images/dy play one color.png" alt="logo"></div>

  <form action="/authentication/sign-up" method="POST" enctype="multipart/form-data">
    <div>
      <div>
        <label for="input-name">Name</label>
        <input type="text" id="input-name" name="name" placeholder="Your Name" />
        <label for="input-email">Email</label>
        <input type="email" id="input-email" name="email" placeholder="Your Email" />
        <label for="input-password">Password</label>
        <input type="password" name="password" id="input-password" placeholder="A Strong Password" />
      </div>
      <div>
        <label for="input-picture">Profile picture:<br></label>
        <input type="file" accept="image/*" id="input-picture" name="picture" />
      </div>
    </div>
    <div>
      <h3>Game You Play</h3>
      <div >
        {{#each gameList as |item|}}
        <div >
          <input name="{{item}}" type="checkbox" id="inlineCheckbox">
          <label for="inlineCheckbox">{{item}}</label>
        </div>
        {{/each}}
      </div>
    </div>
    <div>
      {{>map}}
    </div>
    <div>
    <label for="inputDescription">About you:</label>
    <textarea name="description" id="inputDescription" cols="30" rows="10"></textarea>
    </div>
    <button>Sign Up</button>
  </form>
  </div>