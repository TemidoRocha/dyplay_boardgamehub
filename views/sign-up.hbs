        <div class="formInline">
          <input class="form-check-input" type="checkbox" id="inlineCheckbox" value="option17">
          <label class="form-check-label" for="inlineCheckbox">Cranium</label>
        </div>
        <div class="formInline">
          <input class="form-check-input" type="checkbox" id="inlineCheckbox" value="option18">
