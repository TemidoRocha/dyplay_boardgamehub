        <div class="formInline">
          <input class="form-check-input" type="checkbox" id="inlineCheckbox" value="option4">
          <label class="form-check-label" for="inlineCheckbox">Qwirkle</label>
        </div>
        <div class="formInline">
          <input class="form-check-input" type="checkbox" id="inlineCheckbox" value="option5">
          <label class="form-check-label" for="inlineCheckbox">Risk</label>
        </div>
        <div class="formInline">
          <input class="form-check-input" type="checkbox" id="inlineCheckbox" value="option6">
          <label class="form-check-label" for="inlineCheckbox">Axis and Allies</label>
        </div>
        <div class="formInline">
          <input class="form-check-input" type="checkbox" id="inlineCheckbox" value="option7">
          <label class="form-check-label" for="inlineCheckbox">Cluedo</label>
        </div>
        <div class="formInline">
          <input class="form-check-input" type="checkbox" id="inlineCheckbox" value="option8">
          <label class="form-check-label" for="inlineCheckbox">Battleship</label>
        </div>
        <div class="formInline">
          <input class="form-check-input" type="checkbox" id="inlineCheckbox" value="option9">
          <label class="form-check-label" for="inlineCheckbox">Jenga</label>
        </div>
        <div class="formInline">
          <input class="form-check-input" type="checkbox" id="inlineCheckbox" value="option10">
          <label class="form-check-label" for="inlineCheckbox">Trivial Pursuit</label>
        </div>
        <div class="formInline">
          <input class="form-check-input" type="checkbox" id="inlineCheckbox" value="option11">
          <label class="form-check-label" for="inlineCheckbox">Twister</label>
        </div>
        <div class="formInline">
          <input class="form-check-input" type="checkbox" id="inlineCheckbox" value="option12">
          <label class="form-check-label" for="inlineCheckbox">Blokus</label>
        </div>
        <div class="formInline">
          <input class="form-check-input" type="checkbox" id="inlineCheckbox" value="option13">
          <label class="form-check-label" for="inlineCheckbox">Connect Four</label>
        </div>
        <div class="formInline">
          <input class="form-check-input" type="checkbox" id="inlineCheckbox" value="option14">
